---
config:
  flowchart:
    curve: linear
---
graph TD;
	__start__([<p>__start__</p>]):::first
	reflection(reflection)
	backtest(backtest)
	pnl_plot(pnl_plot)
	__end__([<p>__end__</p>]):::last
	__start__ --> reflection;
	backtest --> reflection;
	reflection -.-> __end__;
	reflection -.-> backtest;
	reflection -.-> pnl_plot;
	pnl_plot --> __end__;
	classDef default fill:#f2f0ff,line-height:1.2
	classDef first fill-opacity:0
	classDef last fill:#bfb6fc
